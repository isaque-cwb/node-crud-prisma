
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model Product{
  id              Int       @id @default(autoincrement())
  name            String    
  bar_code        String    @unique
  price           Float     
  created_at      DateTime  @default(now()) 

  @@map("products")

}


model Category{
  id Int @id @default(autoincrement())
  name String

  @@map("categories")
}
